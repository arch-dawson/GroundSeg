#!/bin/bash

cd /home/dawson/testFiles/

str="testFile$(date +%s)"

touch str

headerStr=$(od -A n -t x1 header)

headerStr=${headerStr//[[:space:]]/}

headerStr=${headerStr:0:30}

#echo $headerStr >> /home/dawson/testFiles/$str

data=$(openssl rand -hex 120)

#echo $data >> /home/dawson/testFiles/$str

#echo $headerStr >> /home/dawson/testFiles/$str

outStr="$headerStr$data$headerStr"

echo $outStr >> /home/dawson/testFiles/$str

#tr -d '\n' < /home/dawson/testFiles/$str




